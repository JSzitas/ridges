glm_acc <- function( train_y, test_y, train_X, test_X ) {
df <- data.frame( y = train_y, train_X )
fit <- glm( y~.-1, data = df, family = binomial )
return(coef(fit))
pred <- c(predict( fit, as.data.frame(test_X), type = "response"))
acc( test_y, pred )
}
